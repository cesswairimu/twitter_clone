<% provide(:title, "Users") %>
<h1> Users</h1>
<table>
    <tr>
        <th>Name </th>
        <th>Email </th>
        <th>Joined at </th>
    </tr>



    <% @users.each do |user| %>
        <tr>
            <td> <%= user.jina %></td>
            <td> <%= user.pepe %></td>
            <td> <%= user.created_at %></td>
        </tr>        
    <% end %>
</table>
